ionalComponent string =\"don't they?\" function = {toggleBackground} selectedStyle={backgroundColor} />\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default PropsDemo;\r\n\r\nconst FunctionalComponent = (props) =>{\r\n    return (\r\n        <div>\r\n            <p>{props.string}</p>\r\n            <button onClick={props.function}>Press Me!!</button>\r\n            <TinyComponent selectedStyle={props.selectedStyle} />\r\n        </div>\r\n    )\r\n}\r\n\r\nconst TinyComponent = (props) => {\r\n    return(\r\n        <div>\r\n            <p>The current style is : {props.selectedStyle}</p>\r\n        </div>\r\n    )\r\n}\r\n\r\nFunctionalComponent.defaultProps = {\r\n    string:'This is wild!',\r\n    function: () => console.log('Can I see this in my dev tools?'),\r\n    selectedStyle: 'what style??'\r\n};\r\n\r\nFunctionalComponent.propTypes = {\r\n    string: PropTypes.string.isRequired,\r\n    function: PropTypes.func.isRequired,\r\n    selectedStyle: PropTypes.string.isRequired\r\n}\r\n\r\n","C:\\Users\\mstra\\Desktop\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\concepts\\Hooks.js",["91"],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":11,"column":25,"nodeType":"98","messageId":"99","endLine":11,"endColumn":98,"fix":"100"},{"ruleId":"96","severity":1,"message":"97","line":16,"column":25,"nodeType":"98","messageId":"99","endLine":16,"endColumn":94,"fix":"101"},{"ruleId":"96","severity":1,"message":"97","line":21,"column":25,"nodeType":"98","messageId":"99","endLine":21,"endColumn":90,"fix":"102"},{"ruleId":"96","severity":1,"message":"97","line":26,"column":25,"nodeType":"98","messageId":"99","endLine":26,"endColumn":111,"fix":"103"},{"ruleId":"96","severity":1,"message":"97","line":31,"column":25,"nodeType":"98","messageId":"99","endLine":31,"endColumn":107,"fix":"104"},{"ruleId":"96","severity":1,"messa